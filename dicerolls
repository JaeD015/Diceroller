import random
#https://www.w3schools.com/python/python_casting.asp
dieRolls= input('How many time would you like to roll?')

dieRollsInteger=int(dieRolls)

diceSides=input ('How many sides would you like?')

diceSidesInteger=int(diceSides)

for rolls in range(dieRollsInteger):
    randomDice = random.randrange(diceSidesInteger)+1
    print(randomDice)

    if diceSidesInteger==20: 
        if randomDice==20:
            print('critical hit')
        if randomDice==1:
            print('critical miss')
    
print('look at you go!')

